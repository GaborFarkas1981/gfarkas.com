{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mainApplication": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/mainApplication",
      "sourceRoot": "projects/mainApplication/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mainApplication",
            "index": "projects/mainApplication/src/index.html",
            "main": "projects/mainApplication/src/main.ts",
            "polyfills": "projects/mainApplication/src/polyfills.ts",
            "tsConfig": "projects/mainApplication/tsconfig.app.json",
            "assets": [
              "projects/mainApplication/src/favicon.ico",
              "projects/mainApplication/src/assets"
            ],
            "styles": [
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/nova/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/mainApplication/src/styles.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/mainApplication/src/environments/environment.ts",
                  "with": "projects/mainApplication/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mainApplication:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mainApplication:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mainApplication:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mainApplication/src/test.ts",
            "polyfills": "projects/mainApplication/src/polyfills.ts",
            "tsConfig": "projects/mainApplication/tsconfig.spec.json",
            "karmaConfig": "projects/mainApplication/karma.conf.js",
            "assets": [
              "projects/mainApplication/src/favicon.ico",
              "projects/mainApplication/src/assets"
            ],
            "styles": [
              "projects/mainApplication/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/mainApplication/tsconfig.app.json",
              "projects/mainApplication/tsconfig.spec.json",
              "projects/mainApplication/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/mainApplication/e2e/protractor.conf.js",
            "devServerTarget": "mainApplication:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mainApplication:serve:production"
            }
          }
        }
      }
    },
    "mediaMarkt": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/media-markt",
      "sourceRoot": "projects/media-markt/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mediaMarkt",
            "index": "projects/media-markt/src/index.html",
            "main": "projects/media-markt/src/main.ts",
            "polyfills": "projects/media-markt/src/polyfills.ts",
            "tsConfig": "projects/media-markt/tsconfig.app.json",
            "assets": [
              "projects/media-markt/src/favicon.ico",
              "projects/media-markt/src/assets"
            ],
            "styles": [
              "projects/media-markt/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/media-markt/src/environments/environment.ts",
                  "with": "projects/media-markt/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "mediaMarkt:build:production"
            },
            "development": {
              "browserTarget": "mediaMarkt:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mediaMarkt:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/media-markt/src/test.ts",
            "polyfills": "projects/media-markt/src/polyfills.ts",
            "tsConfig": "projects/media-markt/tsconfig.spec.json",
            "karmaConfig": "projects/media-markt/karma.conf.js",
            "assets": [
              "projects/media-markt/src/favicon.ico",
              "projects/media-markt/src/assets"
            ],
            "styles": [
              "projects/media-markt/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }},
  "defaultProject": "mainApplication"
}
